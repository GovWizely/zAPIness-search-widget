{
  "name": "zapiness-search-widget",
  "version": "0.0.1",
  "description": "zAPIness search widget",
  "main": "server.js",
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/config/setup.js"
    ]
  },
  "scripts": {
    "lint": "eslint src --fix",
    "start": "node server.js",
    "build": "rm -rf build/* && NODE_ENV=production webpack --config webpack.config.prod.js",
    "deploy": "gh-pages -d build",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "license": "MIT",
  "homepage": "https://govwizely.github.io/zAPIness-search-widget",
  "dependencies": {
