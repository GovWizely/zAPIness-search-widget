    "prop-types": "^15.5.8",
    "radium": "^0.18.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-measure": "^2.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.1.1",
    "redux-form": "^6.3.2",
    "redux-thunk": "^2.1.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
